#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nitish/dev/repositories/rigidui/node_modules/.pnpm/shadcn@2.4.0-canary.12_@types+node@20.19.9_typescript@5.8.3/node_modules/shadcn/dist/node_modules:/home/nitish/dev/repositories/rigidui/node_modules/.pnpm/shadcn@2.4.0-canary.12_@types+node@20.19.9_typescript@5.8.3/node_modules/shadcn/node_modules:/home/nitish/dev/repositories/rigidui/node_modules/.pnpm/shadcn@2.4.0-canary.12_@types+node@20.19.9_typescript@5.8.3/node_modules:/home/nitish/dev/repositories/rigidui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nitish/dev/repositories/rigidui/node_modules/.pnpm/shadcn@2.4.0-canary.12_@types+node@20.19.9_typescript@5.8.3/node_modules/shadcn/dist/node_modules:/home/nitish/dev/repositories/rigidui/node_modules/.pnpm/shadcn@2.4.0-canary.12_@types+node@20.19.9_typescript@5.8.3/node_modules/shadcn/node_modules:/home/nitish/dev/repositories/rigidui/node_modules/.pnpm/shadcn@2.4.0-canary.12_@types+node@20.19.9_typescript@5.8.3/node_modules:/home/nitish/dev/repositories/rigidui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shadcn/dist/index.js" "$@"
else
  exec node  "$basedir/../shadcn/dist/index.js" "$@"
fi
